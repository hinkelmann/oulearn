{
    "name": "hinkelmann/oulearn",
    "type": "project",
    "license": "GPL-3.0-or-later",
    "description": "Core framework that implements the functionality of the Oulearn e-learning platform",
    "homepage": "https://oulearn.org/",
    "keywords": [
        "symfony",
        "oulearn",
        "open learning",
        "ubiquos learning",
        "e-learning",
        "web-application",
        "platform",
        "data-managment"
    ],
    "authors": [
        {
            "name": "Oulearn Community",
            "homepage": "https://github.com/hinkelmann/oulearn/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/hinkelmann/oulearn/issues",
        "source": "https://github.com/hinkelmann/oulearn",
        "docs": "https://docs.oulearn.org/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-bcmath": "*",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "amphp/http-client": "^4.6",
        "dantleech/phpcr-migrations-bundle": "^1.3",
        "doctrine/annotations": "^1.2",
        "doctrine/cache": "^1.0",
        "doctrine/collections": "^1.0",
        "doctrine/data-fixtures": "^1.3.3",
        "doctrine/dbal": "^2.13 || ^3.0",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/inflector": "^1.4.1 || ^2.0.1",
        "doctrine/orm": "^2.13",
        "doctrine/persistence": "^2.0",
        "doctrine/phpcr-bundle": "^2.2",
        "dragonmantank/cron-expression": "^1.1 || ^2.0 || ^3.0",
        "friendsofphp/proxy-manager-lts": "^1.0",
        "friendsofsymfony/http-cache": "^2.10",
        "friendsofsymfony/http-cache-bundle": "^2.10.1",
        "friendsofsymfony/jsrouting-bundle": "^2.7 || ^3.0",
        "friendsofsymfony/rest-bundle": "^3.0.4",
        "gedmo/doctrine-extensions": "^3.0.4",
        "guzzlehttp/promises": "^1.0",
        "handcraftedinthealps/goodby-csv": "^1.4",
        "handcraftedinthealps/rest-routing-bundle": "^1.0",
        "imagine/imagine": "^1.0",
        "jackalope/jackalope": "^1.4",
        "jackalope/jackalope-jackrabbit": "^1.4",
        "jms/serializer": "^3.16",
        "jms/serializer-bundle": "^3.8 || ^4.0",
        "massive/build-bundle": "^0.5",
        "massive/search-bundle": "^2.6.4",
        "matomo/device-detector": "^3.7 || ^4.0.1 || ^5.0 || ^6.0",
        "nyholm/psr7": "^1.3",
        "oro/doctrine-extensions": "^1.0.8 || ^2.0",
        "phpcr/phpcr-migrations": "^1.3.1",
        "phpcr/phpcr-utils": "^1.6.2",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.15",
        "ramsey/uuid": "^3.1 || ^4.0",
        "sensio/framework-extra-bundle": "^6.1",
        "stof/doctrine-extensions-bundle": "^1.2.2",
        "symfony-cmf/routing": "^2.3 || ^3.0",
        "symfony-cmf/routing-bundle": "^2.4 || ^3.0",
        "symfony-cmf/slugifier-api": "^2.1",
        "symfony/asset": "6.2.*",
        "symfony/cache": "6.2.*",
        "symfony/config": "6.2.*",
        "symfony/console": "6.2.*",
        "symfony/css-selector": "6.2.*",
        "symfony/dependency-injection": "6.2.*",
        "symfony/doctrine-messenger": "6.2.*",
        "symfony/dom-crawler": "6.2.*",
        "symfony/dotenv": "6.2.*",
        "symfony/event-dispatcher": "^5.4 || ^6.0",
        "symfony/event-dispatcher-contracts": "^2.1 || ^3.0",
        "symfony/expression-language": "6.2.*",
        "symfony/filesystem": "6.2.*",
        "symfony/finder": "6.2.*",
        "symfony/flex": "^2",
        "symfony/form": "6.2.*",
        "symfony/framework-bundle": "6.2.*",
        "symfony/http-client": "6.2.*",
        "symfony/http-foundation": "6.2.*",
        "symfony/http-kernel": "6.2.*",
        "symfony/intl": "6.2.*",
        "symfony/mailer": "6.2.*",
        "symfony/mime": "6.2.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "6.2.*",
        "symfony/options-resolver": "6.2.*",
        "symfony/process": "6.2.*",
        "symfony/property-access": "6.2.*",
        "symfony/property-info": "6.2.*",
        "symfony/proxy-manager-bridge": "6.2.*",
        "symfony/runtime": "6.2.*",
        "symfony/security-bundle": "6.2.*",
        "symfony/security-core": "6.2.*",
        "symfony/security-http": "6.2.*",
        "symfony/serializer": "6.2.*",
        "symfony/string": "6.2.*",
        "symfony/translation": "6.2.*",
        "symfony/translation-contracts": "^3.0",
        "symfony/twig-bundle": "6.2.*",
        "symfony/validator": "6.2.*",
        "symfony/web-link": "6.2.*",
        "symfony/webpack-encore-bundle": "^1.16",
        "symfony/yaml": "6.2.*",
        "toflar/psr6-symfony-http-cache-store": "^3.0 || ^4.0",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0",
        "webmozart/assert": "^1.9"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "bootstrap-test-environment": [
            "@php bin/runtests --initialize --no-bundle-tests --no-component-tests"
        ],
        "lint": [
            "@lint-twig",
            "@lint-yaml",
            "@lint-container",
            "@lint-composer",
            "@lint-doctrine",
            "@php-cs",
            "@phpstan"
        ],
        "test": [
            "Composer\\Config::disableProcessTimeout",
            "@php bin/runtests --all"
        ],
        "phpstan": [
            "@php bin/adminconsole cache:warmup --env=dev",
            "@php vendor/bin/phpstan analyse"
        ],
        "php-cs": "@php vendor/bin/php-cs-fixer fix --verbose --diff --dry-run",
        "php-cs-fix": "@php vendor/bin/php-cs-fixer fix",
        "lint-composer": "@composer validate --strict",
        "lint-twig": "@php bin/console lint:twig templates",
        "lint-yaml": "@php bin/console lint:yaml config",
        "lint-container": [
            "@php bin/console lint:container --env dev",
            "@php bin/console lint:container --env test",
            "@php bin/console lint:container --env stage",
            "@php bin/console lint:container --env prod"
        ],
        "lint-doctrine": [
            "@php bin/console doctrine:schema:validate --skip-sync",
            "@php bin/console doctrine:ensure-production-settings --env prod"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.2.*"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "google/cloud-storage": "^1.0",
        "handcraftedinthealps/zendsearch": "^2.0",
        "jackalope/jackalope-doctrine-dbal": "^1.6",
        "jangregor/phpstan-prophecy": "^1.0",
        "league/flysystem": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0.1",
        "league/flysystem-azure-blob-storage": "^0.1 || ^1.0",
        "matthiasnoback/symfony-config-test": "^4.2",
        "matthiasnoback/symfony-dependency-injection-test": "^4.2",
        "microsoft/azure-storage-blob": "^1.2",
        "monolog/monolog": "^1.26.1 || ^2.3",
        "php-ffmpeg/php-ffmpeg": "^0.17 || ^1.0",
        "phpcr/phpcr-shell": "^1.4.0",
        "phpspec/prophecy": "^1.14",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-doctrine": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpstan/phpstan-symfony": "^1.0",
        "phpstan/phpstan-webmozart-assert": "^1.0",
        "psr/event-dispatcher": "^1.0",
        "scheb/2fa-backup-code": "^6.0",
        "scheb/2fa-bundle": "^6.0",
        "scheb/2fa-email": "^6.0",
        "scheb/2fa-google-authenticator": "^6.0",
        "scheb/2fa-totp": "^6.0",
        "scheb/2fa-trusted-device": "^6.0",
        "superbalist/flysystem-google-storage": "^7.1",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "6.2.*",
        "symfony/css-selector": "6.2.*",
        "symfony/debug-bundle": "6.2.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^6.2",
        "symfony/runtime": "^6.2",
        "symfony/stopwatch": "6.2.*",
        "symfony/var-dumper": "6.2.*",
        "symfony/web-profiler-bundle": "6.2.*",
        "thecodingmachine/phpstan-strict-rules": "^1.0"
    }
}
